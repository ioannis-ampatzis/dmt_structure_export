{
  "name": "ioannis-ampatzis/dmt_structure_export",
  "description": "Drush commands to export D7 site structure.",
  "type": "drupal-drush",
  "keywords": ["drupal", "drush", "automation"],
  "require": {
    "php": ">=5.6"
  },
  "require-dev": {
    "drupal/coder": "^8.2",
    "drupal/drupal": "^7.62",
    "drush/drush": "^8.1.18",
    "phpro/grumphp": "^0.14",
    "squizlabs/php_codesniffer": "^2.9"
  },
  "autoload": {
    "psr-4": {
      "Drush\\dmt_structure_export\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/7"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable" : true
}
